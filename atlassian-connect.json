{
  "key": "linked-issues-validator",
  "name": "Linked Issues Validator",
  "baseUrl": "https://kaushiqbattu.github.io/linked-issues",
  "authentication": {
    "type": "jwt"
  },
  "scopes": [ "READ", "WRITE", "ADMIN" ],
  "modules": {
    "jiraWorkflowValidators": [
      {
        "key": "linked-issues-match-jql",
        "name": {
          "value": "All linked issues match JQL"
        },
        "description": {
          "value": "Blocks the transition unless every linked issue satisfies the JQL you supply."
        },
        "url": "/validate", 
        "function": "validate-handler",
        "configurationUrl": "/create.html",
        "view": {
          "url": "/view.html"
        }
      }
    ]
  }
}